{
	"extends": "next/core-web-vitals",
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"no-duplicate-imports": "error",
		"no-template-curly-in-string": "warn",
